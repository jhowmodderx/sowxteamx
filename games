local gameLoaders = {
    -- voxseas
    ["104067066727140"] = "https://raw.githubusercontent.com/jhowmodderx/sowxteamx/refs/heads/main/sowxteam",
   

   -- forsaken
    ["18687417158"] = "https://raw.githubusercontent.com/jhowmodderx/sowxteamx/refs/heads/main/forsaken",

    -- 99 Nights
    ["79546208627805"]  = "https://raw.githubusercontent.com/jhowmodderx/sowxteamx/refs/heads/main/99nightssowxteam",
    ["126509999114328"] = "https://raw.githubusercontent.com/jhowmodderx/sowxteamx/refs/heads/main/99nightssowxteam",
    
    -- fisch
    ["16732694052"]     = "https://raw.githubusercontent.com/jhowmodderx/sowxteamx/refs/heads/main/fisch",
    ["131716211654599"] = "https://raw.githubusercontent.com/jhowmodderx/sowxteamx/refs/heads/main/fisch"
}

local gameId = tostring(game.PlaceId)
local loaderUrl = gameLoaders[gameId]

if loaderUrl then
    loadstring(game:HttpGet(loaderUrl))()
else
    game.Players.LocalPlayer:Kick("‚ùå Nenhum loader encontrado para este jogo.")
end
