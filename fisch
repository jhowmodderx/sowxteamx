    Title = "AUTO CAST",
    Default = false
})

AutoCastToggle:OnChanged(function(value)
    Settings.AutoCast = value
    spawn(function()
        while Settings.AutoCast do
            AutoCast()
            task.wait(1)
        end
    end)
end)

