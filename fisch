                if v.Name == "Treasure Map" then
                    LocalPlayer.Character.Humanoid:EquipTool(v)
                    workspace.world.npcs["Jack Marrow"].treasure.repairmap:InvokeServer()
                    Fluent:Notify({
                        Title = "Mapa",
                        Content = "Mapa reparado com sucesso!",
                        Duration = 3
                    })
                    break
                end
            end
        end)
    end
})



